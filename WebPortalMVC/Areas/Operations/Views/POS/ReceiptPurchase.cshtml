@model Domain.DTO.Operations.POS.Output.ReceiptPurchaseOutputDto
@{
    ViewData["Title"] = "PointOfSale";
    Layout = "~/Views/Shared/_Layout.cshtml";
} 
 
<div class="receipt-purchase p-4 container">
    <div style="width: 100%;height: auto;" id="reciept-tags">
        <div style="height: auto;display: flex;width: 100%;">
            <img style="height: 60px;object-fit: contain;" src="data:image/svg+xml;base64,"/>
            <div style="margin-right: auto;margin-left: auto;">
                <h3 style="font-family: Montserrat;font-size: 24px;text-align: center;"><b>SUPREME HERBS INTL.</b></h3>
                <p style="text-align: center;">72-C Tramo St., Gahak, Kawit, Cavite</p>
            </div>
            <h4 style="font-family: Montserrat;font-size: 24px;">
                <b>RECEIPT </b>
            </h4>
        </div>
        <br/>
        <br/>
        <div style="height: auto;display: flex;width: 100%;">
            <ul style="padding-left: 0px;margin-right: auto;">
                <ol style="padding-left: 0px;"><b>Bill To</b></ol>
                @if (Model.distributor_details != null)
                {
                    <ol style="padding-left: 0px;">@Model.distributor_details.first_name @Model.distributor_details.last_name</ol>
                    <ol style="padding-left: 0px;">@Model.distributor_details.complete_address</ol>
                    <ol style="padding-left: 0px;">+63 @Model.distributor_details.mobile_number</ol>
                }
                
            </ul>
            <table>
                <tr>
                    <td><b>Receipt #</b></td>
                    <td style="padding-left: 24px;">@(Model.transaction.tran_number + Model.transaction.ident)</td>
                </tr>
                <tr>
                    <td><b>Receipt Date</b></td>
                    <td style="padding-left: 24px;">@Model.transaction.created_dt.ToString("MMMM dd, yyyy")</td>
                </tr>
                <tr>
                    <td><b>Time</b></td>
                    <td style="padding-left: 24px;">@Model.transaction.created_dt.ToString("h:mm tt")</td>
                </tr>
                <tr>
                    <td><b>Processed By</b></td>
                    <td style="padding-left: 24px;">@Model.transaction.created_by_name</td>
                </tr> 
            </table>
        </div>
        <br/>
        <div style="width: 100%;min-height: 300px;">
            <table style="border-collapse: collapse;width: 100%;">
                <thead>
                <tr>
                    <th style="border: 1px solid #ddd;padding: 8px;font-weight: 600;">Quantity</th>
                    <th style="border: 1px solid #ddd;padding: 8px;font-weight: 600;">PRODUCT</th>
                    <th style="border: 1px solid #ddd;padding: 8px;font-weight: 600;">SRP</th>
                    <th style="border: 1px solid #ddd;padding: 8px;font-weight: 600;">DISC. Price</th>
                    <th style="border: 1px solid #ddd;padding: 8px;font-weight: 600;">Amount</th>
                </tr>
                </thead>
                <tbody>
                @{
                    decimal totalAmount = 0;
                }
                @foreach (var pos in Model.posTransactions)
                {
                    totalAmount += (pos.unit_price * pos.quantity);
                    <tr>
                        <td style="border: 1px solid #ddd;padding: 8px;">@pos.quantity</td>
                        <td style="border: 1px solid #ddd;padding: 8px;">@pos.product_details.p_name</td>
                        <td style="border: 1px solid #ddd;padding: 8px;">₱ @pos.srp_price.ToString("N2")</td>
                        <td style="border: 1px solid #ddd;padding: 8px;">₱ @pos.unit_price.ToString("N2")</td>
                        <td style="border: 1px solid #ddd;padding: 8px;font-weight: 600;">₱ @((pos.unit_price * pos.quantity).ToString("N2"))</td>
                    </tr>
                }
                <tr>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td style="border: 1px solid #ddd;padding: 8px;font-weight: 600;background-color: #CECECE;">TOTAL</td>
                    <td style="border: 1px solid #ddd;padding: 8px;font-weight: 600;background-color: #CECECE;">₱ @totalAmount.ToString("N2")</td>
                </tr>
                </tbody>
            </table>
        </div>
        <br/>
        <br/>
        <div style="width: 100%;text-align: center;">
            <p style="text-align: center;">Thank you for Purchase</p>
        </div>
    </div>
    <hr/>
    <button class="btn btn-primary" id="btn-download">Download Receipt</button>
    <a href="@("/Operations"+Url.Action("Index","POS"))" class="btn btn-outline-dark" style="width: 160px;margin-top: 8px;">Go to POS</a>
</div>


<script>
    $(document).ready(() => {
        authenticateOperationsPage(() => {});
    })
    
    $("#btn-download").on('click', function (){
        generatePDFDownload("@Model.distributor_details?.last_name" + "_" + "@Model.distributor_details?.first_name" +"_" + "@(Model.transaction.tran_number + Model.transaction.ident)", $("#reciept-tags").html());
    })
    
    function generatePDFDownload(fileName, htmlContent){
        const element = htmlContent;
        html2pdf()
            .set({
                filename: `${fileName}.pdf`,
                margin: [10, 10, 10, 10],
                image: { type: 'jpeg', quality: 0.98 },
                html2canvas: { scale: 2, logging: true },
                jsPDF: { unit: 'mm', format: 'a4', orientation: 'portrait' }
            })
            .from(element)
            .save(); 
    }
</script>
